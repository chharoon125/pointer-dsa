#include<iostream>
using namespace std;
int main(){
	int a=2;
	int*b = &a;
	cout<<"the  adrees of a "<<&a<<endl;
	cout<<"the  adrees of b "<<b<<endl;
	cout<<"the value at adrees of b "<<*b<<endl;

	int ** c=&b;
	
	cout<<"the  adrees of b " << &b<<endl;
	cout<<"the  adrees of b " << b<<endl;
	cout<<"the value at adrees value at (value at (c)) of b "<<*b<<endl;
	
	return 0;
}
